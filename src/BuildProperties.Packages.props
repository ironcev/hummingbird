<!-- Build properties used in projects whose outputs are NuGet packages. -->
<Project>
  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.0</Version>
    <Authors>ironcev</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/ironcev/hummingbird</PackageProjectUrl>
    <RepositoryUrl></RepositoryUrl>
  </PropertyGroup>
</Project>